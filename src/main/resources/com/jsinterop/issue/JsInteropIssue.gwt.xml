<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='jsinteropissue'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.logging.Logging"/>

    <inherits name="gwt.material.design.GwtMaterialWithJQueryDebug"/>
    <inherits name="com.workingflows.js.jquery.JQuery"/>

    <!-- Specify the paths for translatable code -->
    <source path='client'/>

    <!-- Specify the app entry point class -->
    <entry-point class='com.jsinterop.issue.client.JsInteropIssueEntry'/>

    <add-linker name="xsiframe"/>
</module>
